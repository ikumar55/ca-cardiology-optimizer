# Cardiology Care Optimization System - Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# API Keys (Required for data collection and ML operations)
# =============================================================================

# CMS NPPES API (for provider data)
CMS_API_KEY=your_cms_api_key_here

# CDC PLACES API (for health data)
CDC_API_KEY=your_cdc_api_key_here

# Google Maps API (for geocoding and travel times - fallback option)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# US Census API (for demographic data)
CENSUS_API_KEY=your_census_api_key_here

# =============================================================================
# Machine Learning APIs (for enhanced model training)
# =============================================================================

# OpenAI API (for research and documentation assistance)
OPENAI_API_KEY=your_openai_api_key_here

# Weights & Biases (for experiment tracking)
WANDB_API_KEY=your_wandb_api_key_here

# =============================================================================
# Cloud Storage and Computing
# =============================================================================

# AWS (for data storage and compute)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-west-2

# Google Cloud Platform (alternative)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/gcp-key.json
GCP_PROJECT_ID=your_gcp_project_id

# =============================================================================
# Database Configuration
# =============================================================================

# DuckDB database file location
DUCKDB_PATH=./data/processed/cardiology_db.duckdb

# =============================================================================
# Application Configuration
# =============================================================================

# Environment (development, staging, production)
ENVIRONMENT=development

# Debug mode
DEBUG=true

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Streamlit configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost

# =============================================================================
# Model Configuration
# =============================================================================

# Random seed for reproducibility
RANDOM_SEED=42

# Model artifacts directory
MODEL_ARTIFACTS_PATH=./models

# Maximum memory usage (in GB)
MAX_MEMORY_GB=8

# =============================================================================
# Security Settings
# =============================================================================

# Secret key for application security
SECRET_KEY=your_secret_key_here

# HTTPS settings (for production)
USE_HTTPS=false
SSL_CERT_PATH=""
SSL_KEY_PATH=""
